{
  "name": "RARBG",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrentapi.org"
  ],
  "base_url": "https://torrentapi.org",
  "token": {
    "query": "/pubapi_v2.php?get_token=get_token&app_id=weyd",
    "token_validity_time_ms": 840000,
    "token_format": {
      "token": "token"
    }
  },
  "movie": {
    "query": "/pubapi_v2.php?mode=search&search_imdb={query}&category=movies&format=json_extended&app_id=weyd&token={token}",
    "keywords": "{imdbId}"
  },
  "episode": {
    "query": "/pubapi_v2.php?mode=search&search_string={query}&category=tv&format=json_extended&app_id=weyd&token={token}",
    "keywords": "{title} {episodeCode}"
  },
  "season": {
    "query": "/pubapi_v2.php?mode=search&search_string={query}&category=tv&format=json_extended&app_id=weyd&token={token}",
    "keywords": [
      "{title} {seasonCode}",
      "{title} season"
    ]
  },
  "more_pages_indicator": "<li class=\"last\">",
  "next_page_path" : "div.pagination ul li.active + li a[href]",
  "next_page_path_attr": "abs:href",
  "links_on_first_page": false,
  "drill_down_path": "tbody tr td a[href^=/torrent]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.page-content",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "div.box-info-heading h1",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "div.torrent-detail-page div:eq(1) div ul:eq(0) li:eq(0) a",
  "magnet_path_attr": "href",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "div.torrent-detail-page div:eq(1) div ul:eq(1) li:eq(3)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
