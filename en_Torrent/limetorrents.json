{
    "name": "limetorrents",
    "language": ["hi", "en"],
    "domains": ["limetorrents.info", "limetorrents.unblockit.ltd", "limetorrents.buzz", "limetorrent.ws"],
    "base_url": "https://www.limetorrents.info",
    "search_url_format_episode": {
      "string_format": "/search/tv/%s+s%02de%02d/1/",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/search/movies/%s+%04d/1/",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "more_pages_indicator": "<ul class=\"pagination\">",
    "next_page_path" : "ul.pagination li.active + li a",
    "next_page_path_attr": "href",
    "links_on_first_page": false,
    "drill_down_path": "span.tv3 +a",
    "drill_down_path_attr": "abs:href",
    "is_torrent": true,
    "is_direct": false,
    "links_container_path": "a[href^=magnet]",
    "link_path": "",
    "link_path_attr": "",
    "title_path": "/dl:contains(name) dd",
    "title_path_attr": "",
    "details_path": "|title_path|",
    "details_path_attr": "",
    "magnet_path": "a[href^=magnet]",
    "magnet_path_attr": "href",
    "seeders_path": "/dl:contains(swarm) dd b:eq(0)",
    "seeders_path_attr": "",
    "filesize_path": "/dl:contains(size) dd",
    "filesize_path_attr": "",
    "quality_path": "|title_path|",
    "quality_path_attr": "",
    "info_path": "|title_path|",
    "info_path_attr": ""
  }
  
